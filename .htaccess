<IfModule mod_headers.c>
    <FilesMatch "\.(pdf|bmp|cur|gif|ico|jpe?g|png|svgz?|webp)$">
        Header set Access-Control-Allow-Origin "*"
    </FilesMatch>
</IfModule>

